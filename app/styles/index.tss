'Window': {
	backgroundColor: '#fff'
}

'VideoPlayer' : {
	autoplay: true,
	width: Ti.UI.FILL,
	height: Ti.UI.FILL,
	showsControls: true
}

'VideoPlayer[platform=ios]' : {
	allowsAirPlay: true,
	backgroundColor: 'transparent',
	mediaTypes: [ Ti.Media.VIDEO_MEDIA_TYPE_VIDEO ]
}

'VideoPlayer[platform=android]' : {
	keepScreenOn: true,
	backgroundColor: '#000'
}
